!!! 5
%html
  %head
    %title 
      Викторины
    = stylesheet_link_tag "application", :media => "all"
    = javascript_include_tag "application"
    = csrf_meta_tags
  %body
    .container
      = flash_messages
      .content.nowrapped
        .left-panel
          = render template: 'users/info'
          .menu
            = render template: 'quizzes/menu'
        .middle-panel
          .title
            .text
              = yield(:page_title)
          = yield